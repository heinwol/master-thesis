{
    "python.defaultInterpreterPath": "${workspaceFolder}/.nix-env/bin/python",
    "python.analysis.exclude": [
        "submodules",
        ".direnv",
        "result",
        ".nix-env"
    ],
    "cSpell.enabled": true,
    "cSpell.language": "ru,en",
    "cSpell.enableFiletypes": [
        "typst",
        "!python"
    ],
    "cSpell.diagnosticLevel": "Information",
    "cSpell.maxNumberOfProblems": 50,
    "cSpell.words": [
        "ctheorems",
        "delim",
        "Dynkin",
        "forall",
        "gost",
        "Kuznetsov",
        "linebreak",
        "nonum",
        "pagebreak",
        "thmbox",
        "thmproof",
        "thmrules",
        "typst",
        "underbrace",
        "Zhilyakova",
        "графовая",
        "многоагентные",
        "стабилизирующейся",
        "эйлеровых"
    ],
}
