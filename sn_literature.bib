@PhdThesis{ЖИЛЯКОВА2013,
  author   = {ЖИЛЯКОВА Л.Ю.},
  school   = {Институт проблем управления им. В.А. Трапезникова Российской академии наук},
  title    = {РЕСУРСНЫЕ СЕТИ И АНАЛИЗ ИХ ДИНАМИКИ},
  year     = {2013},
  month    = oct,
  type     = {Thesis},
  abstract = {Цель: исследование ресурсных сетей, предельных состояний и потоков в них при любой топологии, любом количестве ресурса и его начальном распределении по вершинам, их качественных и количественных характеристик; выявление классов сетей, в которых предельные состояния существуют и зависят от начальных, постановка и решение задач управления в таких сетях. Сформулированы и доказаны теоремы о существовании и единственности порогового значения Т для каждого класса сетей. Доказаны теоремы о предельных состояниях и потоках для всех классов, об аттракторах и о величине порога в каждом классе сетей. Получены новые результаты, касающиеся алгебраических свойств нерегулярных стохастических матриц. Найдена зависимость предельного состояния в поглощающих сетях с несколькими стоками от начального распределения ресурса. Сформулированы две задачи управления ресурсом в множестве целевых вершин. Обоснованы методы сведения обеих задач к задачам оптимизации. Доказана выпуклость целевой функции. Разработана м},
  url      = {https://www.elibrary.ru/item.asp?id=22346860},
}

 
